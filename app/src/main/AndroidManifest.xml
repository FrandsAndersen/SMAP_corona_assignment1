<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coronaStats">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_corona_stats_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_corona_stats_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.coronaStats.ListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.coronaStats.EditActivity" />
        <activity android:name="com.example.coronaStats.DetailsActivity"/>
    </application>

</manifest>